# Redirector Environment Variables Example
# Copy this file to .env.local and update the values

# API URL for local development
REDIRECTOR_API_URL=http://localhost:8787

# Cloudflare KV namespace ID (used by CLI for direct KV operations)
REDIRECTOR_KV_NAMESPACE_ID=your-kv-namespace-id

# Cloudflare account ID (used by CLI for direct Cloudflare API operations)
CLOUDFLARE_ACCOUNT_ID=your-account-id

# Log level (trace, debug, info, warn, error, fatal)
LOG_LEVEL=info

# Enable debug mode
ENABLE_DEBUG=false